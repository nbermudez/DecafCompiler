class Fibonacci {
	int potato;
	int fibonacci(int x) {
		int f1, f2,x;
		if (x == 0 || x == 1) {
			return 1;
		} else {
			f1 = fibonacci(x-1);
			print "fibonacci ", x-1, " = ", f1, "\n";

			f2 = fibonacci(x-2);
			print "fibonacci ", x-2, " = ", f2, "\n";
			//f1 = fibonacci(x-1);
			//f2 = fibonacci(x-2);
			//f1 = f1 + f2;
			//potato = 20;
			//return fibonacci(x-1)+fibonacci(x-2);
			return f1 + f2;
		}
	}

	void main() {
		int t;
		t = fibonacci(5);
		print t;
	}
}